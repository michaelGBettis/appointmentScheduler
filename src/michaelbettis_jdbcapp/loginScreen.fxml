      <TextField fx:id="usernameBox" layoutX="10.0" layoutY="71.0" prefWidth="400.0" promptText="Enter Username" />
      <PasswordField fx:id="passwordBox" layoutX="10.0" layoutY="106.0" prefHeight="25.0" prefWidth="400.0" promptText="Enter Password" />
      <Button fx:id="loginButton" layoutX="110.0" layoutY="141.0" mnemonicParsing="false" onAction="#loginButtonHandler" prefHeight="25.0" prefWidth="200.0" text="Login" />
      <Label fx:id="signInLabel" layoutX="10.0" layoutY="10.0" prefHeight="53.0" prefWidth="242.0" text="Sign In">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
